general: {
	debug_level = 4
	log_to_stdout = true
}

certificates: {
	cert_pem = "/opt/janus/share/janus/certs/mycert.pem"
	cert_key = "/opt/janus/share/janus/certs/mycert.key"
}

media: {
	ipv6 = false
	min_nack_queue = 500
	rtp_port_range = "10000-10200"
}

nat: {
	nice_debug = false
	ice_ignore_list = "vmnet,10.0.0.1"
}

plugins: {
	disable = "libjanus_voicemail.so,libjanus_recordplay.so"
}

transports: {
	disable = "libjanus_rabbitmq.so,libjanus_mqtt.so,libjanus_nanomsg.so"
}
